      "inherited": false,
      "mediaGroups": ["visual"] },
    { "key": "border-right", "as": "border-top" },
    { "key": "border-bottom", "as": "border-top" },
    { "key": "border-left", "as": "border-top" },
    { "key": "border-top-color",
